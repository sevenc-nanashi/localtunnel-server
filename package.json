  },
  "dependencies": {
    "@alexjamesmalcolm/human-readable-ids": "1.0.12",
    "debug": "4.3.4",
    "esm": "3.2.25",
    "koa": "2.14.1",
    "koa-router": "12.0.0",
    "localenv": "0.2.2",
    "log4js": "6.7.1",
    "optimist": "0.6.1",
    "pump": "3.0.0",
    "tldjs": "2.3.1",
    "ts-mixer": "6.0.2"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/koa": "2.13.5",
    "@types/koa-router": "7.4.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.18",
    "@types/optimist": "0.0.30",
    "@types/pump": "1.1.1",
    "@types/supertest": "2.0.12",
    "@types/tldjs": "2.3.1",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "mocha": "10.2.0",
    "nodemon": "2.0.20",
    "prettier": "2.8.2",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "ws": "8.12.0"
  },
  "scripts": {
    "test": "tsc --noEmit && mocha --check-leaks --exit --require ts-node/register **/*.test.ts",
    "start": "tsc -p tsconfig.no-test.json && node ./dist/server.js --port 5000",
    "dev": "nodemon --exec \"ts-node index.ts --port 5000\" --ext ts,js,json",
    "build": "tsc -p tsconfig.no-test.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  }
}
